\r\n\t\t\t\tconst res: any = await CalendarApi.update({ ...param, ID: ID })\r\n\t\t\t\tif (res.status == 200 || res.status == 204) {\r\n\t\t\t\t\tshowNoti('success', 'Thành công')\r\n\t\t\t\t\tgetData()\r\n\t\t\t\t}\r\n\t\t\t} catch (error) {\r\n\t\t\t\tshowNoti('danger', error?.message)\r\n\t\t\t} finally {\r\n\t\t\t\tsetLoading(false)\r\n\t\t\t}\r\n\t\t}\r\n\r\n\t\tconst _createRoom = async (data) => {\r\n\t\t\tsetLoading(true)\r\n\t\t\ttry {\r\n\t\t\t\tconst res = await CalendarApi.createRoom(data?.id)\r\n\t\t\t\tres.status == 200 && getData()\r\n\t\t\t} catch (error) {\r\n\t\t\t\tshowNoti('danger', error?.message)\r\n\t\t\t} finally {\r\n\t\t\t\tsetLoading(false)\r\n\t\t\t}\r\n\t\t}\r\n\r\n\t\tconst join = async (dataID) => {\r\n\t\t\tconst res = await fetchInfoRoomZoom(dataID)\r\n\t\t\tres == true && window.open(`/course/zoom-view/${event?.id}/?isVideo=true`)\r\n\t\t}\r\n\r\n\t\tconst content = (\r\n\t\t\t<div className=\"course-dt-event-info\" style={{ maxWidth: 300, minWidth: 200 }}>\r\n\t\t\t\t<ul>\r\n\t\t\t\t\t{event?.name && (\r\n\t\t\t\t\t\t<li>\r\n\t\t\t\t\t\t\t<span>Tiêu đề:</span> {event?.name}\r\n\t\t\t\t\t\t</li>\r\n\t\t\t\t\t)}\r\n\r\n\t\t\t\t\t{checkDate > -1 && (\r\n\t\t\t\t\t\t<>\r\n\t\t\t\t\t\t\t{Status == 1 && (\r\n\t\t\t\t\t\t\t\t<>\r\n\t\t\t\t\t\t\t\t\t{isTeach() ? (\r\n\t\t\t\t\t\t\t\t\t\t<>\r\n\t\t\t\t\t\t\t\t\t\t\t<Button\r\n\t\t\t\t\t\t\t\t\t\t\t\tsize=\"middle\"\r\n\t\t\t\t\t\t\t\t\t\t\t\tclassName=\"btn-warning w-100\"\r\n\t\t\t\t\t\t\t\t\t\t\t\tonClick={() => {\r\n\t\t\t\t\t\t\t\t\t\t\t\t\tsetEventSelected(event)\r\n\t\t\t\t\t\t\t\t\t\t\t\t\tsetEditvisible(true)\r\n\t\t\t\t\t\t\t\t\t\t\t\t}}\r\n\t\t\t\t\t\t\t\t\t\t\t>\r\n\t\t\t\t\t\t\t\t\t\t\t\tCập nhật\r\n\t\t\t\t\t\t\t\t\t\t\t</Button>\r\n\t\t\t\t\t\t\t\t\t\t\t<DeleteEvent title={event?.title} Status={Status} ID={event?.id} date={event?.start} getData={getData} />\r\n\t\t\t\t\t\t\t\t\t\t</>\r\n\t\t\t\t\t\t\t\t\t) : (\r\n\t\t\t\t\t\t\t\t\t\t<>\r\n\t\t\t\t\t\t\t\t\t\t\t<BookEvent title