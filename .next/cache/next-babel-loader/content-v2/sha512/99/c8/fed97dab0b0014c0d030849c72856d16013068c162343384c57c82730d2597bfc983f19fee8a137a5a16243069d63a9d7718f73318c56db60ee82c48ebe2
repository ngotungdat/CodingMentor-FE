I\n          }\n        } else if (Status == 3) {\n          color = '#2f7ed4'; // ĐÃ BOOK\n        } else if (Status == 4) {\n          color = '#4AB876'; // ĐÃ HOÀN THÀNH\n        } else if (Status == 5) {\n          color = '#EF5DA8'; // ĐÃ XÁC NHẬN DẠY\n        } else if (Status == 6) {\n          color = '#ab5713'; // LỖI ĐỘT XUẤT\n        } else if (Status == 7) {\n          color = '#1ccdcb'; // ĐANG ZOOM\n        } else if (Status == 8) {\n          color = '#495ECA'; // ĐÃ KẾT THÚC\n        } else if (Status == 9) {\n          color = '#172B4D'; // ĐÃ TẠO PHÒNG\n        } else {\n          color = _.primaryColor;\n        }\n      }\n\n      return color;\n    };\n\n    return /*#__PURE__*/_jsxDEV(\"div\", {\n      className: \"calendar-item\",\n      onClick: function onClick(e) {\n        e.stopPropagation();\n        e.nativeEvent.stopImmediatePropagation();\n      },\n      style: {\n        backgroundColor: getColor(),\n        height: '100%'\n      },\n      children: /*#__PURE__*/_jsxDEV(Popover, {\n        zIndex: 999,\n        title: /*#__PURE__*/_jsxDEV(\"div\", {\n          className: \"w-100 popover-title\",\n          children: [\"\".concat(event === null || event === void 0 ? void 0 : event.title, \" \").concat(checkDate == 0 ? ' [Hôm nay]' : ''), /*#__PURE__*/_jsxDEV(\"div\", {\n            children: /*#__PURE__*/_jsxDEV(ModalHistory, {\n              ID: event === null || event === void 0 ? void 0 : event.id,\n              title: event === null || event === void 0 ? void 0 : event.title\n            }, void 0, false, {\n              fileName: _jsxFileName,\n              lineNumber: 476,\n              columnNumber: 9\n            }, _this)\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 475,\n            columnNumber: 8\n          }, _this)]\n        }, void 0, true, {\n          fileName: _jsxFileName,\n          lineNumber: 473,\n          columnNumber: 7\n        }, _this),\n        content: